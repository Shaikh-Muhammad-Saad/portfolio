{
  "name": "ygency-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier -c .",
    "lint:fix": "eslint --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "prepare": "husky install && chmod ug+x .husky/*",
